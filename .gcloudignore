# Ignore node modules
node_modules/
web/node_modules/

# Ignore Python cache
__pycache__/
*.pyc
*.pyo
*.pyd
.Python
*.so
*.egg
*.egg-info/
dist/
build/

# Ignore git
.git/
.gitignore

# Ignore IDE
.vscode/
.idea/
*.swp
*.swo

# Ignore env files (keep them local)
.env
.env.local

# DO NOT ignore data files - we need them in the container!
!data/
!data/processed/
!data/processed/*.parquet
!data/enriched/
!data/enriched/*.parquet
!data/raw/

# Ignore raw CSV files (too large, we use parquet)
data/raw/*.csv
